{
  "name": "wordle",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --collect-coverage",
    "lint": "eslint .",
    "start": "npm run open",
    "open": "open GUI/index.html"
  },
  "author": "",
  "license": "ISC",
  "eslintConfig": {
    "globals": {
      "expect": true,
      "Wordle": true
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "mocha": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 9
    },
    "rules": {
      "eqeqeq": "error",
      "strict": "error",
      "no-var": "error",
      "no-unused-vars": [
        "off"
      ],
      "prefer-const": "error",
      "no-console": "off",
      "indent": [
        "error",
        2
      ],
      "quotes": [
        "error",
        "single",
        "avoid-escape"
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  },
  "devDependencies": {
    "eslint": "^8.9.0",
    "jest": "^27.5.1"
  }
}